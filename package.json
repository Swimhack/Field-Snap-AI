{
  "name": "field-snap-ai",
  "version": "0.2.0",
  "description": "Field Snap AI Web App - Lead processing pipeline with OCR and data enrichment",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production PORT=8080 node dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Tests coming soon\" && exit 0",
    "deploy": "fly deploy"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.0.2",
    "@supabase/supabase-js": "^2.39.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.7",
    "tesseract.js": "^6.0.1",
    "twilio": "^4.20.0",
    "web-push": "^3.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "ocr",
    "lead-processing",
    "image-analysis",
    "typescript",
    "web-app"
  ],
  "author": "Field Snap AI Team",
  "license": "MIT"
}
