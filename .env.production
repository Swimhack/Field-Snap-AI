# Field Snap AI - Production Environment Variables
# Copy this to .env.local and fill in your actual values

# Application
NODE_ENV=production
PORT=8080

# Database (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_BUCKET_RAW=leads-raw
SUPABASE_BUCKET_OCR=leads-ocr
SUPABASE_BUCKET_THUMBS=leads-thumbs

# OCR Providers
GOOGLE_APPLICATION_CREDENTIALS=./infra/gcp.json
GOOGLE_MAPS_API_KEY=your-maps-key
OPENAI_API_KEY=sk-your-openai-key

# Communication
TWILIO_ACCOUNT_SID=your-twilio-sid
TWILIO_AUTH_TOKEN=your-twilio-token
TWILIO_FROM_NUMBER=+1234567890
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-secret
GMAIL_REFRESH_TOKEN=your-refresh-token

# Deployment
NETLIFY_AUTH_TOKEN=your-netlify-token
NETLIFY_SITE_NAME_PREFIX=fieldsnap-

# Security
API_BEARER_TOKEN=your-secure-admin-token
ALLOW_SEND=false

# Performance
API_RATE_LIMIT_RPM=60
OCR_TIMEOUT_MS=10000
ENRICHMENT_TIMEOUT_MS=5000